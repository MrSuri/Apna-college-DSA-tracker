{
  "name": "dsa-sheet-microservices",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start:gateway": "cd backend/api-gateway && npm start",
    "start:auth": "cd backend/auth-service && npm start",
    "start:content": "cd backend/content-service && npm start",
    "start:progress": "cd backend/progress-service && npm start",
    "start:backend": "npm-run-all --parallel start:gateway start:auth start:content start:progress",
    "start:frontend": "cd frontend && npm start"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}


